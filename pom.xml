	<groupId>org.ylzl</groupId>
	<artifactId>eden-practice-java</artifactId>
	<packaging>pom</packaging>
	<name>practice</name>
	<version>1.0.0-SNAPSHOT</version>
	<description>Eden* Practice For Java</description>

	<modules>
		<module>eden-practice-algorithms</module>
		<module>eden-practice-design-pattern</module>
		<module>eden-practice-dubbo</module>
		<module>eden-practice-kafka</module>
		<module>eden-practice-mybatis</module>
		<module>eden-practice-redis</module>
		<module>eden-practice-spring-boot</module>
		<module>eden-practice-spring-framework</module>
		<module>eden-practice-protobuf</module>
	</modules>

	<distributionManagement>
		<repository>
			<id>github</id>
			<name>GitHub OWNER Apache Maven Packages</name>
			<url>https://maven.pkg.github.com/eden-lab/eden-spring-boot</url>
		</repository>
	</distributionManagement>

	<developers>
		<developer>
			<id>gyl</id>
			<name>guoyuanlu</name>
			<email>shiyindaxiaojie@gmail.com</email>
			<organization>ylzl</organization>
			<organizationUrl>https://github.com/eden-lab</organizationUrl>
			<roles>
				<role>DevOps</role>
			</roles>
		</developer>
	</developers>

	<licenses>
		<license>
			<name>Apache License, Version 2.0</name>
			<url>https://www.apache.org/licenses/LICENSE-2.0</url>
		</license>
	</licenses>

	<scm>
		<url>https://github.com/eden-lab/eden-practice-java</url>
		<connection>scm:git:git://github.com/eden-lab/eden-practice-java.git</connection>
		<developerConnection>scm:git:ssh://git@github.com/eden-lab/eden-practice-java.git
		</developerConnection>
	</scm>

	<repositories>
		<repository>
			<id>github</id>
			<name>GitHub OWNER Apache Maven Packages</name>
			<url>https://maven.pkg.github.com/eden-lab/eden-spring-boot</url>
		</repository>
	</repositories>

	<build>
		<plugins>
			<plugin>
				<groupId>org.apache.maven.plugins</groupId>
				<artifactId>maven-resources-plugin</artifactId>
			</plugin>
			<plugin>
				<groupId>org.apache.maven.plugins</groupId>
				<artifactId>maven-compiler-plugin</artifactId>
			</plugin>
			<plugin>
				<groupId>org.apache.maven.plugins</groupId>
				<artifactId>maven-jar-plugin</artifactId>
			</plugin>
		</plugins>
	</build>

	<dependencies>
		<dependency>
			<groupId>cglib</groupId>
			<artifactId>cglib</artifactId>
		</dependency>
		<dependency>
			<groupId>com.fasterxml.jackson.core</groupId>
			<artifactId>jackson-annotations</artifactId>
